{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "no-console": [false, "log", "error"],
        "no-empty": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "ordered-imports": false,
        "trailing-comma": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}